package kr.co.itcruit.vo;

import java.util.Date;

import lombok.Data;

@Data
public class AlarmVO {
	
	private int alarmNo;
	private String alarmRcvr;
	private String alarmSndr;
	private String readYn;
	private String alarmCn; 
	private String alarmUrl;
	private Date alarmTime;
	private String alarmCtgry;
	private String alarmTtl;
	
	
	
	private boolean isPass;
	
	public AlarmVO() {};
	
	public AlarmVO(String alarmRcvr, String alarmSndr, String alarmCn, Date alarmTime, String alarmCtgry, String alarmTtl){
		
		this.alarmRcvr = alarmRcvr;
		this.alarmSndr = alarmSndr;
		this.alarmCn = alarmCn;
		this.alarmTime = alarmTime;
		this.alarmCtgry = alarmCtgry;
		this.alarmTtl = alarmTtl;
		
		
	}
	
	public AlarmVO(String alarmRcvr, String alarmSndr, String alarmCn, Date alarmTime, String alarmCtgry, String alarmTtl, boolean isPass){
		
		this.alarmRcvr = alarmRcvr;
		this.alarmSndr = alarmSndr;
		this.alarmCn = alarmCn;
		this.alarmTime = alarmTime;
		this.alarmCtgry = alarmCtgry;
		this.alarmTtl = alarmTtl;
		this.isPass = isPass;
		
	}
}
