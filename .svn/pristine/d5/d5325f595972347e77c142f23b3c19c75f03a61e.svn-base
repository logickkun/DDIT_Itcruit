					<!-- 라디오 버튼 스크립트. -->
					<script>

						document.addEventListener("DOMContentLoaded", function(){

							var checkboxContainer = document.querySelector(".checkbox-container");

							var radioList = document.querySelectorAll(".checkboxlist");
							
							radioList.forEach(function(checkList){
								
								var checkRadio = checkList.querySelector("input[type='radio']:checked");
								
// 								console.log(checkRadio);
								
								if(checkRadio) {
									
									esntlAjax(checkRadio);
									
								}
								
								
								checkList.addEventListener("change", function(){
									
									var selectedRadio = checkList.querySelector("input[type='radio']:checked");
									
									esntlAjax(selectedRadio);
									
								});
								
							})
							
							function esntlAjax(selectedRadio){
								
								checkboxContainer.innerHTML = "";
								
								//AJAX
								var xhr = new XMLHttpRequest();
									
								xhr.open("GET", "/myPage/enterprise/esntLangList?pstnCmmncdNm=" + encodeURIComponent(selectedRadio.id), true);
								
								xhr.onreadystatechange = function() {
									
									if(xhr.readyState === XMLHttpRequest.DONE){
										
										if(xhr.status === 200){
											
// 											console.log("요청 성공");

											var responseData = JSON.parse(xhr.responseText);
// 											console.log(responseData);
											
											responseData.forEach(function(e){
												
												const upperCase = e.cmmncdNm1.charAt(0).toUpperCase() + e.cmmncdNm1.slice(1);
												
												var checkbox = createCheckedbox(upperCase, upperCase);
					                            checkboxContainer.appendChild(checkbox);
											});
											
											upCheckedBox();
											
											
										}else{
											
// 											console.log("요청 실패");
											
										}
										
										
									}
									
								}
								
								
								
								xhr.send();
								
							}

						});
						
						
						function upCheckedBox(){
							
							var upCheckBoxOne = document.querySelector(".upCheckBoxOne");
							var upCheckBoxTwo = document.querySelector(".upCheckBoxTwo");
							var upCheckBoxThree = document.querySelector(".upCheckBoxThree");
							var upCheckBoxFour = document.querySelector(".upCheckBoxFour");
							var upCheckBoxFive = document.querySelector(".upCheckBoxFive");
							
					        var checkedInput1 = upCheckBoxOne.querySelector('input[type="radio"]:checked');
					        var checkedInput2 = upCheckBoxTwo.querySelector('input[type="radio"]:checked');
					        var checkedInput3 = upCheckBoxThree.querySelector('input[type="radio"]:checked');
					        var checkedInput4 = upCheckBoxThree.querySelector('input[type="checkbox"]:checked');
					        var checkedInput5 = upCheckBoxThree.querySelector('input[type="checkbox"]:checked');
					        
					        if (checkedInput1) {
					        	
					        	upCheckBoxOne.prepend(checkedInput1.parentElement);
					        	
