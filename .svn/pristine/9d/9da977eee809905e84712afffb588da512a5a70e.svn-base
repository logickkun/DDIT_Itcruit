 	<select id="getProgressRecruit" resultType="map">
		SELECT 
		    R.RECRUIT_TTL,
		    R.RECRUIT_NO,
		    R.PSTN_CMMNCD_NM,
		    APL.CLS_NM,
		    APL.PROCESS_SEQ,
		    APL.STRT_YMD,
		    APL.PRSNTN_YMD,
		    EPA.EACH_ORDER,
		    COUNT(EPA.EACH_PROCESS_NO) AS COUNT
		FROM 
		    RECRUIT R
		JOIN 
		    APPLY_PROCESS_LIST APL ON R.RECRUIT_NO = APL.RECRUIT_NO AND R.PSTN_CMMNCD_NM = APL.PSTN_CMMNCD_NM
		LEFT JOIN 
		    EACH_PROCESS_APPLY_PASS EPA ON APL.PROCESS_SEQ = EPA.PROCESS_SEQ
		WHERE 
		    R.ENT_NO = #{entNo}
		    AND CURRENT_DATE BETWEEN APL.STRT_YMD AND APL.PRSNTN_YMD
		GROUP BY 
		    R.RECRUIT_TTL,
		    R.RECRUIT_NO,
		    R.PSTN_CMMNCD_NM,
		    APL.CLS_NM,
		    APL.PROCESS_SEQ,
		    APL.STRT_YMD,
		    APL.PRSNTN_YMD,
		    EPA.EACH_ORDER
		 	
 	</select>
 	
