 	<select id="getDeadLineRecruit" resultType="recruitVO">
 		SELECT
		    r.recruit_no,
		    r.recruit_ttl,
		    r.recruit_strt_dt,
		    r.recruit_end_dt,
		    r.recruit_yn,
		    r.recruit_career_code_nm,
		    r.recruit_career_dt,
		    r.ent_url,
		    r.recruit_hit,
		    r.process_yn,
		    r.process_amt,
		    r.recruit_pay,
		    r.recruit_enroll_ymd,
		    r.cmmncd_grp_cd,
		    r.cmmncd_nm1,
		    r.recruit_cnt,
		    r.pstn_cmmncd_nm,
		    r.ent_postcode,
		    r.ent_nm,
		    r.ent_no,
		    r.ent_apply,
		    <![CDATA[
		    LISTAGG(el.esntl_lang_cmmncd_nm, ', ') WITHIN GROUP (ORDER BY el.esntl_lang_cmmncd_nm) AS esntlLangCmmncdNm,
		    TRUNC(TO_DATE(r.recruit_end_dt, 'YYYY-MM-DD"T"HH24:MI') - TRUNC(SYSDATE)) AS deadLineDay
		    ]]>
		FROM
		    recruit r
		LEFT JOIN
		    ESNTL_LANG el ON r.recruit_no = el.recruit_no
		WHERE
		    r.ent_no = #{entNo}
		    <![CDATA[
		    AND TRUNC(TO_DATE(r.recruit_end_dt, 'YYYY-MM-DD"T"HH24:MI') - TRUNC(SYSDATE)) <= 3
    		AND TRUNC(TO_DATE(r.recruit_end_dt, 'YYYY-MM-DD"T"HH24:MI') - TRUNC(SYSDATE)) >= 0
    		]]>
		GROUP BY
		    r.recruit_no,
		    r.recruit_ttl,
		    r.recruit_strt_dt,
		    r.recruit_end_dt,
		    r.recruit_yn,
		    r.recruit_career_code_nm,
		    r.recruit_career_dt,
		    r.ent_url,
		    r.recruit_hit,
		    r.process_yn,
		    r.process_amt,
		    r.recruit_pay,
		    r.recruit_enroll_ymd,
		    r.cmmncd_grp_cd,
		    r.cmmncd_nm1,
		    r.recruit_cnt,
		    r.pstn_cmmncd_nm,
		    r.ent_postcode,
		    r.ent_nm,
		    r.ent_no,
		    r.ent_apply
		ORDER BY
		    deadLineDay ASC
 	
 	</select>
 	
 	
 	
