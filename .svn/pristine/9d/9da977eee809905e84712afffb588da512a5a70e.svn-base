		DELETE FROM HIRE_STTS
		
		WHERE
		
		    RECRUIT_NO = #{recruitNo} 
		    
		    AND
		    
		    PSTN_CMMNCD_NM = #{pstnCmmncdNm}
		    
	</delete>
	
	
	<insert id="recruitPreferLangInsert" parameterType="recruitVO">
	
