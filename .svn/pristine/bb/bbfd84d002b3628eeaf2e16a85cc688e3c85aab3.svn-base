<!-- 		<put-attribute name="content" value="/WEB-INF/views/codingtest/{1}.jsp"/> -->
<!-- 	</definition> -->
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	<!-- 김나혜-->
	<definition name="memberPage/*" extends="memberPage">
		<put-attribute name="content" value="/WEB-INF/views/member/{1}.jsp"/>
	</definition>
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	<!-- 김영상-->
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	<!-- 신하림 -->
	<definition name="admin/*" extends="adminPage">
		<put-attribute name="content" value="/WEB-INF/views/admin/{1}.jsp"/>
	</definition>
	
	
	<definition name="footerMenu/*" extends="index">
		<put-attribute name="content" value="/WEB-INF/views/footerMenu/{1}.jsp"/>
	</definition>
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
