var careerCheckbox = document.querySelector("input[name='careers.0_proceeding']");
//경험 진행중 체크박스 id
var expCheckbox = document.querySelector("input[name='etcHistories.0_proceeding']");

//체크박스 체크 시 뒤에 연도,월 비활성
function handleCheckboxChange() {

    console.log("handleCheckboxChange 함수 탐");

    var checked_opt = $(this).is(":checked");
    console.log("^^",checked_opt);
    
    var careerSecondCalendarBox = document.querySelectorAll(".calendar_box")[1];
    var careerYearInput = careerSecondCalendarBox.querySelector(".calendar_year");
    var careerMonthInput = careerSecondCalendarBox.querySelector(".calendar_month");
    
    var expSecondCalendarBox = document.querySelectorAll(".calendar_box")[3];
    var expYearInput = expSecondCalendarBox.querySelector(".calendar_year");
    var expMonthInput = expSecondCalendarBox.querySelector(".calendar_month");

    if (careerCheckbox.checked) {
        console.log("career on");
        careerSecondCalendarBox.classList.add("read_only");
        careerYearInput.disabled = true;
        careerMonthInput.disabled = true;

    }else if(expCheckbox.checked) {
        console.log("exp on");
        expSecondCalendarBox.classList.add("read_only");
        expYearInput.disabled = true;
        expMonthInput.disabled = true;

    }else if(checked_opt === false) {
        if($(this)[0].name == "careers.0_proceeding") {
            console.log("career off");
            careerSecondCalendarBox.classList.remove("read_only");
            careerYearInput.disabled = false;
            careerMonthInput.disabled = false;
        }
        if($(this)[0].name == "etcHistories.0_proceeding") {
            console.log("exp off");
            expSecondCalendarBox.classList.remove("read_only");
            expYearInput.disabled = false;
            expMonthInput.disabled = false;
        }
    }
}

careerCheckbox.addEventListener("change", handleCheckboxChange);
expCheckbox.addEventListener("change", handleCheckboxChange);


