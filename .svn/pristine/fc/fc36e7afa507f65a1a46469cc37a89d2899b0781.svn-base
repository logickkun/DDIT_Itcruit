	<insert id="insertResumeLang" parameterType="availLangVO">
		INSERT INTO AVAIL_LANG_LIST (
			AVAIL_LANG_SEQ
			, AVAIL_LANG_CMMNCD_NM
			, RSM_NO
			
		) VALUES (
			SEQ_RESUME_LANG.nextval
			, #{availLangCmmncdNm}
			, #{rsmNo}
		)
	</insert>
	
