	<insert id="insertResume" parameterType="resumeVO" useGeneratedKeys="true">
		<selectKey keyProperty="rsmNo" resultType="String" order="BEFORE">
			select SEQ_RESUME.nextval from dual
		</selectKey>
		insert into resume (
			rsm_no
			, account_id
			, rsm_gender
			, rsm_ttl
			, rsm_new_career
			, rsm_career_year
			, rsm_rls_yn
		) values (
			#{rsmNo}
			, #{accountId}
			, #{rsmGender}
			, #{rsmTtl}
			, #{rsmNewCareer}
			, #{rsmCareerYear}
			, #{rsmRlsYn}
		)
	</insert>
	
	<select id="langList" parameterType="map" resultType="commoncodeVO">
		SELECT DISTINCT
			cmmncd_nm1
		FROM
		    commoncode
		WHERE
		    CMMNCD_GRP_CD LIKE 'PSTN%'
		AND
			TO_NUMBER(SUBSTR(CMMNCD_GRP_CD, 5)) <![CDATA[<]]>= 14
		AND
		    initcap(CMMNCD_NM1) LIKE initcap('%'||#{value}||'%')
		ORDER BY
            CMMNCD_NM1
	</select>
	
	<select id="selectMember" parameterType="string" resultType="accountVO">
		select
			account_id
			, account_pswd
			, account_nm
			, gender
			, birth_ymd
			, account_nick
			, account_mail
			, account_tell
			, account_zip
			, account_addr1
			, account_addr2
			, enabled
			, auth_cmmncd
			, join_ymd
			, account_stts_cmmncd
			, TO_CHAR(birth_ymd, 'yyyy') birthY
		from 
			account
		where 
			account_id = #{accountId}
	</select>

</mapper>